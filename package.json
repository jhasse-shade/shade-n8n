{
  "name": "n8n-nodes-shade",
  "version": "0.1.0",
  "description": "Interact with your shade workspace via the api",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Jacob Hasselbring",
    "email": "jacob@shade.inc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shade-labs/n8n-nodes-shade.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/ShadeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Shade/Shade.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "n8n-node-dev": "^2.0.0-rc.2",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
